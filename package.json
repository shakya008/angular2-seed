{
  "name": "",
  "description": "",
  "version": "1.0.0",
  "scripts": {
    "prebuild": "npm run clean",
    "build": "cross-env NODE_ENV=production webpack",
    "compodoc": "./node_modules/.bin/compodoc src -p ./tsconfig.json",
    "cover": "istanbul report --include=**/chrome/coverage-final.json text text-summary",
    "dev": "cross-env NODE_ENV=development webpack-dev-server -d --inline --hot --progress --no-info",
    "clean": "rimraf build/",
    "killNode": "sudo sh ./killNode.sh",
    "lint": "npm run lint-ts && npm run lint-css",
    "lint-css": "stylelint './src/**/*.css';true",
    "lint-ts": "tslint 'src/**/*.ts';true",
    "production": "npm install;npm run build; npm run server:prod",
    "servedocs": "./node_modules/.bin/compodoc -s",
    "server:prod": "npm run killNode;cross-env NODE_ENV=production node server/node-server.js > stdout.txt 2> stderr.txt &",
    "start": "webpack-dev-server --inline --port=3334 --content-base build/",
    "test": "node --max-old-space-size=4096 ./node_modules/karma/bin/karma start karma.conf.js --singleRun true",
    "test:watch": "node --max-old-space-size=8192 ./node_modules/karma/bin/karma start karma.conf.js --singleRun false",
    "tslint-count": "npm run lint-ts | wc -l"
  },
